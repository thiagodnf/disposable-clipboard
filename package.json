{
    "name": "disposable-clipboard",
    "version": "0.0.1",
    "description": "Web-based disposable clipboard tool that allows you to rapidly move texts between devices. Just type anything you want, define the expiration time, and share it",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon --ignore 'db.json' server.js",
        "lint": "npx eslint .",
        "lint-fix": "npx eslint . --fix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/thiagodnf/disposable-clipboard"
    },
    "bugs": {
        "url": "https://github.com/thiagodnf/disposable-clipboard/issues"
    },
    "keywords": [
        "clipboard",
        "disposable",
        "web-app"
    ],
    "author": "Thiago Ferreira",
    "license": "MIT",
    "homepage": "https://disposable-clipboard.herokuapp.com",
    "devDependencies": {
        "nodemon": "^2.0.6"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "csurf": "^1.11.0",
        "dayjs": "^1.9.7",
        "ejs": "^3.1.5",
        "eslint": "^7.15.0",
        "express": "^4.17.1",
        "express-validator": "^6.8.0",
        "helmet": "^4.2.0",
        "http-errors": "^1.7.2",
        "lowdb": "^1.0.0",
        "morgan": "^1.10.0",
        "nanoid": "^3.1.20",
        "node-uuid": "^1.4.8",
        "qrcode": "^1.4.4",
        "validator": "^13.5.2",
        "winston": "^3.3.3"
    }
}
